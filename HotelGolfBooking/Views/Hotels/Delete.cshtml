@model HotelGolfBooking.Models.hotel

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<h3>Bạn có chắc chắn xóa khách sạn này?</h3>
<fieldset>
    <label for="room" style="width:auto;display:block;">Khách sạn @Model.name, địa chỉ: @Model.address</label>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" class="btn btn-info" value="Xóa" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}
