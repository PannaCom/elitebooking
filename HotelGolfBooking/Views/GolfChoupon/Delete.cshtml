@model HotelGolfBooking.Models.golf_choupon

@{
    ViewBag.Title = "Delete";
}
<h3>Bạn có chắc chắn xóa choupon này?</h3>
<fieldset>
    <label for="room" style="width:auto;display:block;">Golf choupon giá @HotelGolfBooking.Config.formatNumber((int)@Model.chouponprice)</label>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Create", new { id=Model.idgolf})
    </p>
}
