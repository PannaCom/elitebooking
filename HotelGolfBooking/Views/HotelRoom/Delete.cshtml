@model HotelGolfBooking.Models.hotel_room

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<h3>Bạn có chắc chắn xóa phòng này khỏi dữ liệu?</h3>
<fieldset>
     <div class="col-md-6">
              <div class="form-group">
                <label for="room">Phòng: @Model.roomname, diện tích: @Model.square, giá @Model.price</label>                
              </div>
     </div>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" class="btn btn-info" value="Xóa" /> |
        @Html.ActionLink("Back to List", "Create")
    </p>
}
